{
  "manifest_version": 3,
  "name": "BookNotify",
  "description": "Real-time booking notifications for movies and events.",
  "version": "1.0",
  "permissions": [
    "notifications",
    "activeTab"
  ],
  "background": {
    "service_worker": "src/background.js"
  },
  "action": {
    "default_popup": "src/popup.html"
  }
}
