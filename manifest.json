{
  "manifest_version": 3,
  "name": "BookNotify",
  "description": "Real-time booking notifications for movies and events.",
  "version": "1.0",
  "permissions": [
    "notifications",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "assets/icons/icon16.png",
    "48": "assets/icons/icon48.png",
    "128": "assets/icons/icon128.png"
  }
}
